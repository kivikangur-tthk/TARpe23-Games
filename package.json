  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kivikangur-tthk/TARpe23-Games/issues"
  },
  "homepage": "https://github.com/kivikangur-tthk/TARpe23-Games#readme",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "subpackage": "^1.1.0"
  },
  "overrides": {
    "cross-spawn": "^6.0.6"
  }
}
