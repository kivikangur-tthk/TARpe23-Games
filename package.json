{
  "name": "tarpe23-games",
  "version": "1.0.0",
  "description": "Hajusrakenduste projekt",
  "subPackages": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend && npm run dev",
    "start": "concurrently \"cd backend && npm run start\" \"cd frontend && npm run preview",
    "postinstall": "subpkg install",
    "build": "subpkg run build",
    "backend-swagger": "cd backend && npm run swagger"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kivikangur-tthk/TARpe23-Games.git"
  },
  "keywords": [],
